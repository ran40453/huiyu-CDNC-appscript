{
  "name": "huiyu-cdnc-appscript",
  "version": "1.0.0",
  "description": "Apps Script web app (CDNC form) â€“ managed via VS Code, git, clasp",
  "private": true,
  "scripts": {
    "login": "clasp login",
    "push": "npm run clean:manifest && clasp push",
    "push:raw": "clasp push",
    "clean:manifest": "node -e \"const fs=require('fs');['appsscript.json','.clasp.json'].forEach(f=>{if(fs.existsSync(f)){let t=fs.readFileSync(f,'utf8');t=t.replace(//*[sS]*?*//g,'' ).replace(/(^|s)//.*$/gm,'' ).replace(/,s*([}]])/g,'$1' );fs.writeFileSync(f,t);console.log('cleaned',f);}});\"",
    "ver": "clasp version \"update\"",
    "deploy": "clasp deploy --description \"deploy\"",
    "release": "npm run push && npm run ver && npm run deploy",
    "repo:push": "git push -u origin main"
  },
  "keywords": [
    "google-apps-script",
    "clasp",
    "webapp"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@google/clasp": "^3.0.6-alpha",
    "strip-json-comments": "^5.0.1"
  }
}